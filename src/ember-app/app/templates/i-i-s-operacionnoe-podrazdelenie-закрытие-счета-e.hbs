<h3 class="ui header">{{t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.датаЗакрСчета-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датаЗакрСчета
      class=(if (v-get validationModel "датаЗакрСчета" "isInvalid") "error")
      data-test-i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e-датаЗакрСчета=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаЗакрСчета" "message")}}
  </div>
  <div class="field" data-test-i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e-типСчета="true">
    <label>{{t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.типСчета-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-operacionnoe-podrazdelenie-тип-счета")
      value=model.типСчета
      class=(if (v-get validationModel "типСчета" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "типСчета" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.задолжности-caption"}}</label>
    {{flexberry-checkbox
      readonly=readonly
      value=model.задолжности
      class=(if (v-get validationModel "задолжности" "isInvalid") "error")
      data-test-i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e-задолжности=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "задолжности" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.лицСчета
      class=(if (v-get validationModel "лицСчета" "isInvalid") "error")
      label=(t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.лицСчета-caption")
      data-test-i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e-лицСчета=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "лицСчета" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.организация-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.организация
      displayAttributeName="наименование"
      autocomplete=true
      relationName="организация"
      projection="ОрганизацияL"
      title=(t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.организация-caption")
      readonly=readonly
      componentName="организацияLookup"
      class=(if (v-get validationModel "организация" "isInvalid") "error")
      data-test-i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e-организация=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "организация" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.открытиеСчета-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.открытиеСчета
      displayAttributeName="видВалюты"
      autocomplete=true
      relationName="открытиеСчета"
      projection="ОткрытиеСчетаL"
      title=(t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.открытиеСчета-caption")
      readonly=readonly
      componentName="открытиеСчетаLookup"
      class=(if (v-get validationModel "открытиеСчета" "isInvalid") "error")
      data-test-i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e-открытиеСчета=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "открытиеСчета" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.сотрудники-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.сотрудники
      displayAttributeName="фИО"
      autocomplete=true
      relationName="сотрудники"
      projection="СотрудникиL"
      title=(t "forms.i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e.сотрудники-caption")
      readonly=readonly
      componentName="сотрудникиLookup"
      class=(if (v-get validationModel "сотрудники" "isInvalid") "error")
      data-test-i-i-s-operacionnoe-podrazdelenie-закрытие-счета-e-сотрудники=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сотрудники" "message")}}
  </div>
</form>
